SRC	=	src/main.c	\
		src/cat.c	\
		src/print_tab.c	\
		src/fill_tab_zero_to_four.c	\
		src/fill_tab_five_to_nine.c	\
		src/change_char.c	\
		src/fill_tab.c	\
		src/error.c	\

NAME	=	crocus

VG	=	-g3

ERROR	=	-Wall -W -Wextra

all	:
	gcc -o $(NAME) $(SRC) $(ERROR)

clean	:
	rm -f *.o $(OBJ)

fclean	: clean
	rm -f $(NAME)

re	: fclean all
